@using DinamikCv.Models.Entity
@model List<TblDeneyimlerim>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Deneyim Listesi</h2><br />
<table class="table table-bordered">
    <tr style="background-color:#808080">
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.BASLIK</td>
            <td>@x.ALTBASLIK</td>
            <td>@x.TARIH</td>
            <td><a href="/Deneyim/DeneyimSil/@x.ID" class="btn btn-danger">Sil</a></td>
            <td><a href="/Deneyim/DeneyimGetir/@x.ID" class="btn btn-warning">Düzenle</a></td>
        </tr>
    }
</table>
<a href="/Deneyim/DeneyimEkle/" class="btn btn-primary">Yeni Deneyim Ekle</a>